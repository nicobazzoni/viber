<%= simple_form_for(@club) do |f| %>

  <%= hidden_field_tag "membership[membership_ids][]", nil %>
  <%= f.error_notification %>
  <%= f.input :name %>
  
  <%= f.fields_for :avatar_attributes do |f|%>
   <%= hidden_field_tag "Avatar[avatar_ids][]", nil %>
      <%= f.label :name, "Avatar Name:" %>
      <% Avatar.all.each do |avatar| %>
      <%= avatar.name %>
      <% end %>

  <%= f.error :base %>
  <% end %>
  <%= f.button :submit %>
<% end %>

